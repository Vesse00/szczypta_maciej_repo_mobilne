<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ms_lekarz"
    android:backgroundTint="#77A57952"
    android:backgroundTintMode="src_over"
    tools:layout_editor_absoluteX="42dp"
    tools:layout_editor_absoluteY="0dp">

    <TextView
        android:id="@+id/js_textView_2_2"
        android:layout_width="348dp"
        android:layout_height="55dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="15dp"
        android:fontFamily="serif"
        android:text="lorem ipsum "
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/js_imageview_2"
        app:layout_constraintStart_toStartOf="@+id/js_imageview_2"
        app:layout_constraintTop_toBottomOf="@+id/js_button_login_2" />

    <EditText
        android:id="@+id/editTextTextPersonName5"
        android:layout_width="43dp"
        android:layout_height="41dp"
        android:background="@drawable/ms_buttons"
        android:backgroundTint="@color/white"
        android:ems="10"
        android:fontFamily="serif"
        android:inputType="textPersonName"
        android:text="  OR"
        android:textColor="#3F51B5"
        app:layout_constraintEnd_toEndOf="@+id/js_imageview_2"
        app:layout_constraintStart_toStartOf="@+id/js_imageview_2"
        tools:layout_editor_absoluteY="495dp" />

    <EditText
        android:id="@+id/js_text_email_2"
        android:layout_width="379dp"
        android:layout_height="47dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/ms_buttons"
        android:backgroundTint="#8BEFEBEB"
        android:ems="10"
        android:fontFamily="serif"
        android:inputType="textPersonName"
        android:text="  email"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/js_text_pass_2"
        app:layout_constraintEnd_toEndOf="@+id/js_imageview_2"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/js_imageview_2"
        app:layout_constraintTop_toBottomOf="@+id/js_text_username_2" />

    <EditText
        android:id="@+id/js_text_pass_2"
        android:layout_width="379dp"
        android:layout_height="47dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/ms_buttons"
        android:backgroundTint="#8BEFEBEB"
        android:ems="10"
        android:fontFamily="serif"
        android:inputType="textPersonName"
        android:text="  password"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/js_text_confirm_2"
        app:layout_constraintEnd_toEndOf="@+id/js_imageview_2"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/js_imageview_2"
        app:layout_constraintTop_toBottomOf="@+id/js_text_email_2" />

    <EditText
        android:id="@+id/js_text_confirm_2"
        android:layout_width="379dp"
        android:layout_height="47dp"
        android:background="@drawable/ms_buttons"
        android:backgroundTint="#8BEFEBEB"
        android:ems="10"
        android:fontFamily="serif"
        android:inputType="textPersonName"
        android:text="confirm password"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/js_button_signup_2"
        app:layout_constraintEnd_toEndOf="@+id/js_imageview_2"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/js_imageview_2"
        app:layout_constraintTop_toBottomOf="@+id/js_text_pass_2" />

    <ImageView
        android:id="@+id/js_imageview_2"
        android:layout_width="97dp"
        android:layout_height="103dp"
        android:layout_marginStart="159dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="160dp"
        app:layout_constraintBottom_toTopOf="@+id/js_textView_1_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ms_logo_2" />

    <TextView
        android:id="@+id/js_textView_1_2"
        android:layout_width="236dp"
        android:layout_height="44dp"
        android:layout_marginTop="3dp"
        android:layout_marginBottom="17dp"
        android:fontFamily="serif"
        android:text="SignUp to Yddet"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/js_text_username_2"
        app:layout_constraintEnd_toEndOf="@+id/js_imageview_2"
        app:layout_constraintStart_toStartOf="@+id/js_imageview_2"
        app:layout_constraintTop_toBottomOf="@+id/js_imageview_2" />

    <EditText
        android:id="@+id/js_text_username_2"
        android:layout_width="379dp"
        android:layout_height="47dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/ms_buttons"
        android:backgroundTint="#8BEFEBEB"
        android:ems="10"
        android:fontFamily="serif"
        android:inputType="textPersonName"
        android:text="  your username"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/js_text_email_2"
        app:layout_constraintEnd_toEndOf="@+id/js_imageview_2"
        app:layout_constraintStart_toStartOf="@+id/js_imageview_2"
        app:layout_constraintTop_toBottomOf="@+id/js_textView_1_2" />

    <Button
        android:id="@+id/js_button_signup_2"
        android:layout_width="332dp"
        android:layout_height="41dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="@color/white"
        android:fontFamily="serif"
        android:text="Sign Up"
        android:textAlignment="center"
        android:textColor="#2196F3"
        android:textSize="20sp"
        app:cornerRadius="100px"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextPersonName5"
        app:layout_constraintEnd_toEndOf="@+id/js_imageview_2"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@+id/js_imageview_2"
        app:layout_constraintTop_toBottomOf="@+id/js_text_confirm_2" />

    <Button
        android:id="@+id/js_button_login_2"
        android:layout_width="327dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginBottom="25dp"
        android:backgroundTint="@color/white"
        android:fontFamily="serif"
        android:text="Login"
        android:textColor="#2196F3"
        android:textSize="20sp"
        app:cornerRadius="50px"
        app:layout_constraintBottom_toTopOf="@+id/js_textView_2_2"
        app:layout_constraintEnd_toEndOf="@+id/js_imageview_2"
        app:layout_constraintStart_toStartOf="@+id/js_imageview_2"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName5" />
</androidx.constraintlayout.widget.ConstraintLayout>